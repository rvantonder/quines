$p="";
$c="";
$f="";
sub a{
  @x=unpack("C*",pack("H*",$p));
  print "p=[".join(', ',@x)."]\n";
  @y=unpack("C*",pack("H*",$c));
  print "c=[".join(', ',@y)."]\n";
  @z=unpack("C*",pack("H*",$f));
  print "f=[".join(', ',@z)."]\n";
  print pack('H*',$p)."\n";
}
sub b{
  print "\$p=\"$p\";\n";
  print "\$c=\"$c\";\n";
  print "\$f=\"$f\";\n";
  print pack('H*',$c)."\n";
}
sub c{
  @x=unpack("C*",pack("H*",$c));
  print "let pl = [|".join('; ',@x)."|]\n";
  @y=unpack("C*",pack("H*",$p));
  print "let py = [|".join('; ',@y)."|]\n";
  @z=unpack("C*",pack("H*",$f));
  print "let fs = [|".join('; ',@z)."|]\n";
  print pack('H*',$f)."\n";
}
if (defined($ARGV[0]) && $ARGV[0] eq "py"){
  a;
} elsif (defined($ARGV[0]) && $ARGV[0] eq "fs"){
  c;
} else {
  b;
}
