$p="";
$c="";
sub a{
	@x=unpack("C*",pack("H*",$p));
	print "p=[".join(', ',@x)."]\n";
	@y=unpack("C*",pack("H*",$c));
	print "c=[".join(', ',@y)."]\n";
	print pack('H*',$p)."\n";
}
sub b{
	print "\$p=\"$p\";\n";
	 print "\$c=\"$c\";\n";
	print pack('H*',$c)."\n";
}
if (defined($ARGV[0]) && $ARGV[0] eq "arb"){
	a;
} else {
	b;
}
